{
  "name": "mongo-",
  "config":{
    "connector.class": "com.mongodb.kafka.connect.sink.MongoSinkConnector",
    "config.providers" : "file",
    "config.providers.file.class" : "org.apache.kafka.common.config.provider.FileConfigProvider",
    "connection.uri": "mongodb://${file:/secrets/env.properties:MONGO_INITDB_ROOT_USERNAME}:${file:/secrets/env.properties:MONGO_INITDB_ROOT_PASSWORD}@mongo:29092",
    "database": "testdb",
    "topics.regex": "cdc\\..*",
    "change.data.capture.handler": "com.mongodb.kafka.connect.sink.cdc.debezium.rdbms.mysql.MysqlHandler"
  }
}
